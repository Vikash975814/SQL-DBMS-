Create table CLIENT_MASTER( 
    CLIENT_NO VARCHAR2(6) PRIMARY KEY, 
    NAME VARCHAR2(20) NOT NULL, 
    CITY VARCHAR2(20) CONSTRAINT CITY_CHK CHECK(CITY IN  ('DELHI','MUMBAI','CHANNAI')), 
    PINCODE NUMBER(6), 
    STATES VARCHAR2(20), 
    BALDUE NUMBER(10,2), 
    EMAIL VARCHAR2(30) UNIQUE 
);
